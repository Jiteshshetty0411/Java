        print("~~"*10,"*****THANKS FOR VISITING !***** AND ****** THANKS FOR    YOUR FEEDBACK :) *****","~~"*10)
        print("~~"*10,"*****VISIT AGAIN*******","~~"*10) 
    elif ch=="C":
        print(" NOT BAD BETTER")
        print("~~"*5,"*****THANKS FOR VISITING !****** AND ****** THANKS FOR YOUR FEEDBACK :) *****","~~"*5)
        print("~~"*10,"*****VISIT AGAIN*******","~~"*10) 
    elif ch=="D":
